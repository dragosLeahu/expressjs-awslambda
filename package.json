{
  "name": "expressjs-awslambda",
  "version": "1.0.0",
  "description": "",
  "main": "app.js",
  "scripts": {
    "local": "nodemon ./app.local.js --exec \"npm run lint && node\"",
    "deploy": "claudia create --handler lambda.handler --deploy-proxy-api --version dev --region eu-central-1",
    "update": "claudia update",
    "release": "claudia set-version --version production",
    "lint": "eslint --fix-dry-run **/*.js",
    "start": "ntl"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "claudia": "^5.1.1",
    "eslint": "^5.5.0",
    "eslint-config-airbnb-base": "^13.1.0",
    "eslint-plugin-import": "^2.14.0",
    "nodemon": "^1.18.4",
    "ntl": "^3.0.0"
  },
  "dependencies": {
    "aws-serverless-express": "^3.3.5",
    "dotenv": "^6.0.0",
    "express": "^4.16.3",
    "mongodb": "^3.1.4",
    "morgan": "^1.9.1",
    "standard": "^12.0.1"
  },
  "eslintConfig": {
    "extends": "standard",
    "env": {
      "es6": true,
      "browser": true
    },
    "rules": {
      "no-underscore-dangle": [
        "off"
      ],
      "no-console": [
        "off"
      ]
    }
  }
}